{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-param-reassign": "off",
    "no-restricted-syntax": "off",
    "camelcase": "warn",
    "no-underscore-dangle": "off",
    "padded-blocks": "off"
  }
}